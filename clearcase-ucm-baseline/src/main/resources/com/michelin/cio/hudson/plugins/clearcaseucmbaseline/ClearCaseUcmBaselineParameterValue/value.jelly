<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry title="${%PVOB}">
        <f:textbox name="pvob" value="${it.pvob}" />
    </f:entry>
    <f:entry title="${%Component}">
        <f:textbox name="component" value="${it.component}" />
    </f:entry>
    <f:entry title="${%Promotion level}">
        <f:textbox name="promotionLevel" value="${it.promotionLevel}" />
    </f:entry>
    <f:entry title="${%Baseline}">
        <f:textbox name="baseline" value="${it.baseline}"/>
    </f:entry>
    <f:entry title="${%View name}">
        <f:textbox name="viewName" value="${it.viewName}" />
    </f:entry>
    <f:entry title="${%Snaphot view}">
        <f:textbox name="snapshotView" value="${it.snapshotView}"/>
    </f:entry>
    <f:entry title="${%Use update}">
        <f:textbox name="useUpdate" value="${it.useUpdate}"/>
    </f:entry>
    <f:entry title="${%Force rmview}">
        <f:textbox name="forceRmview" value="${it.forceRmview}"/>
    </f:entry>
</j:jelly>
